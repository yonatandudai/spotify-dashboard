{
  "version": 2,
  "builds": [
    {
      "src": "spotify-profile-demo/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/spotify-profile-demo/dist/$1"
    }
  ],
  "functions": {
    "main.py": {
      "runtime": "python3.9"
    }
  }
}
